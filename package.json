{
  "name": "tiktok-downloader",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:backend": "nodemon --exec ts-node backend/server.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:backend\"",
